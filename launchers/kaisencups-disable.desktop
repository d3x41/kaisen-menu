#!/usr/bin/env xdg-open

[Desktop Entry]
Name=cups-disable
Name[fr]=cups-désactivé
Name[es]=cups-discapacitado
Name[pt]=cups-desativado
Name[it]=cups-disabilitato
Name[de]=cups-behindert
Comment=Disable cups daemons
Comment[fr]=Désactiver services cups
Comment[es]=Desactivar servicios cups
Comment[pt]=Desativar serviços cups
Comment[it]=Disattivare servizios cups
Comment[de]=Services deaktivieren cups
Exec=terminator -e "zsh -c '/opt/services/cups-disable;/bin/zsh'"
Icon=utilities-terminal
Terminal=false
Type=Application
StartupNotify=true
Categories=SCups
X-Kaisen-Package=kaisen-services-management
