#!/usr/bin/env xdg-open

[Desktop Entry]
Name=dislocker-metadata
Comment=Tool for bitlocker disk
Comment[fr]=Outil pour les disques bitlocker
Comment[es]=Herramienta Bitlocker
Comment[pt]=Ferramenta Bitlocker
Comment[it]=Strumento Bitlocker
Comment[de]=Bitlocker-Tool
Exec=/usr/share/kaisen-menu/scripts/kaisen-launcher --command "dislocker-metadata -h"
Icon=utilities-terminal
Terminal=false
Type=Application
StartupNotify=true
Categories=Bitlocker
X-Kaisen-Package=dislocker
