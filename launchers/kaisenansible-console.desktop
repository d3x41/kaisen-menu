#!/usr/bin/env xdg-open

[Desktop Entry]
Name=ansible-console
Comment=Tool to use ansible
Comment[fr]=Outil pour utiliser ansible
Comment[es]=Herramienta Ansible
Comment[pt]=Ferramenta Ansible
Comment[it]=Strumento Ansible  
Comment[de]=Ansible Tool
Exec=terminator -e "zsh -c 'ansible-console -h;/bin/zsh'"
Icon=utilities-terminal
Terminal=false
Type=Application
StartupNotify=true
Categories=Ansible
X-Kaisen-Package=ansible
