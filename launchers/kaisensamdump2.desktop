#!/usr/bin/env xdg-open

[Desktop Entry]
Name=samdump2
Name[fr]=samdump2
Comment=Tool to recover Windows SAM base
Comment[fr]=Outil pour récupérer la base SAM de Windows
Exec=terminator -e "zsh -c 'samdump2 -h;/bin/zsh'"
Icon=utilities-terminal
Terminal=false
Type=Application
StartupNotify=true
Categories=Passwords
X-Kaisen-Package=samdump2
