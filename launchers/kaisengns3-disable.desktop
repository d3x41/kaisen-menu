#!/usr/bin/env xdg-open

[Desktop Entry]
Name=gns3-disable
Name[fr]=gns3-désactivé
Name[es]=gns3-deshabilitado
Name[pt]=gns3-desativado
Name[it]=gns3-disabilitato
Name[de]=gns3-deaktiviert
Comment=Disable gns3 daemon
Comment[fr]=Désactiver service gns3
Comment[es]=Deshabilitar servicio gns3
Comment[pt]=Desativar serviço gns3
Comment[it]=Disabilita servizio gns3
Comment[de]=Deaktivieren Sie den gns3-Dienst
Exec=terminator -e "zsh -c '/opt/services/gns3-disable.sh;/bin/zsh'"
Icon=utilities-terminal
Terminal=false
Type=Application
StartupNotify=true
Categories=SGNS3
X-Kaisen-Package=kaisen-services-management
