#!/usr/bin/xdg-open

[Desktop Entry]
Name=lxd-agent-disable
Name[fr]=lxd-agent-désactivé
Name[es]=lxd-agent-deshabilitado
Name[pt]=lxd-agent-desativado
Name[it]=lxd-agent-disabilitato
Name[de]=lxd-agent-deaktivieren
Comment=Disable lxd-agent daemon
Comment[fr]=Désactiver service lxd-agent
Comment[es]=Deshabilitar servicio lxd-agent
Comment[pt]=Desativar serviço lxd-agent
Comment[it]=Disabilita servizio lxd-agent
Comment[de]=Deaktiviert den lxd-agent-Dienst
Exec=/usr/share/kaisen-menu/scripts/kaisen-launcher --command "/usr/share/kaisen-services-management/lxd-agent-disable" --sudo
Icon=utilities-terminal
Terminal=false
Type=Application
StartupNotify=true
Categories=SContainers
X-Kaisen-Package=lxd-agent
