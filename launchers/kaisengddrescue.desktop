#!/usr/bin/env xdg-open

[Desktop Entry]
Name=gddrescue
Name[fr]=gddrescue
Comment=Tool to recover data on a damaged disk
Comment[fr]=Outil pour récupérer des données sur un disque endommagé
Exec=terminator -e "zsh -c 'sudo ddrescue --help;/bin/zsh'"
Icon=utilities-terminal
Terminal=false
Type=Application
StartupNotify=true
Categories=Recovery
X-Kaisen-Package=gddrescue

