#!/usr/bin/env xdg-open

[Desktop Entry]
Name=sswap
Name[fr]=sswap
Comment=Tool to securely delete swap partition
Comment[fr]=Outil pour effacer de facon sécurisée une partition swap
Exec=terminator -e "zsh -c 'sudo sswap -h;/bin/zsh'"
Icon=utilities-terminal
Terminal=false
Type=Application
StartupNotify=true
Categories=Partitioning
X-Kaisen-Package=secure-delete
